{
    "name": "bojan-samar/inertia-forum",
    "description": "Forum with Laravel, Inertia and Tailwind.",
    "keywords": ["laravel", "tailwind", "forum"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bojan Samardzija",
            "email": "bojan.samar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "ext-json": "*",
        "illuminate/console": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/validation": "^10.0"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^0.6.5"
    },
    "autoload": {
        "psr-4": {
            "BojanSamar\\InertiaForum\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "BojanSamar\\InertiaForum\\InertiaForumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
